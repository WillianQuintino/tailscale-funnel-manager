# Server Configuration
PORT=3002
NODE_ENV=production

# Tailscale Configuration
TAILSCALE_AUTHKEY=tskey-auth-xxxxxxxxxxxxx-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TS_STATE_DIR=/var/lib/tailscale
TS_HOSTNAME=funnel-manager

# Tailscale Subnet Routes (opcional)
# Exemplo: TS_ROUTES=192.168.100.0/24,192.168.200.0/24
TS_ROUTES=

# Tailscale Extra Args (opcional)
# Exemplos:
# - Exit Node: TS_EXTRA_ARGS=--advertise-exit-node
# - Exit Node + SSH: TS_EXTRA_ARGS=--advertise-exit-node --ssh
TS_EXTRA_ARGS=

# Accept DNS and Routes
TS_ACCEPT_DNS=true
TS_ACCEPT_ROUTES=true

# Authentication
# Ativar autenticação com usuário/senha (true/false)
AUTH_ENABLED=true
# Usuário para login (padrão: admin)
AUTH_USERNAME=admin
# Senha para login (padrão: changeme - ALTERE ISSO!)
AUTH_PASSWORD=changeme

# CasaOS Integration (optional)
CASAOS_URL=http://localhost:80

# Next.js
NEXT_TELEMETRY_DISABLED=1
